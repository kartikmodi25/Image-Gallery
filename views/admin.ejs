<% layout('../layouts/boilerplate')%>
<h1>Admin Page</h1>
<table class="table table-striped">
    <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">email</th>
          <th scope="col">password</th>
          <th scope="col">permission</th>
        </tr>
      </thead>

    <% for (let user of users){%>
        <tbody>
        <tr>
            <th scope="row">#</th>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
            <td><%= user.permission %></td>
          </tr>    
        </tbody>    
        <% }%>
    </table>



    
   