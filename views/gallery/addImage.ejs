<% layout('/layouts/boilerplate')%>
<div class="container">
    <div class="row">
        <form action="/welcome/<%= userData._id%>/gallery/add" method="POST" novalidate class="validated-form" enctype="multipart/form-data">
            <div class="mb-3">
                <div class="form-file custom-file">
                    <input type="file" class="form-file-input" id="image" name="image" multiple>
                    <label class="form-file-label" for="image">
                        <span class="form-file-text custom-file-label">Choose image(s)...</span>
                        <span class="form-file-button">Browse</span>
                    </label>
                </div>
            </div>
            <button class="btn btn-success mt-3">Add Image</button> 
        </form>
    </div>
    
</div>
